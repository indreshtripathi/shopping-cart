<div class="food-item">
    <img width="100" height="100" src={{image}}>    
    <div class="description">
        <h3>{{itemName}}</h3>
        {{description}}
    </div>

    <div class="cart-section">
        <button class="add-to-cart">
            <div class="title">Add to cart | ${{price}}</div>
        </button>
    </div>
    <br>
</div>